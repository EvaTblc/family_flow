<%= form_with model: organization, class: "form", local: true do |f| %>
  <%= f.text_field :name, class: "input_form" %>
  <%= f.text_field :address, class: "input_form" %>
  <%= f.submit "Modifier", class: "btn-form" %>

  <%= link_to organization_path(organization) do %>
    <i class="fa-regular fa-circle-xmark fa-xl" style="color: #CB9DF0;"></i>
  <% end %>
  
<% end %>
